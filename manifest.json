{
  "manifest_version": 3,
  "name": "Orbital Solana Assistant",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html",
    "default_title": "Orbital Solana Assistant"
  },
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "host_permissions": [
    "https://api.openai.com/*",
    "https://api.dexscreener.com/*"
  ]
}
